@{
    ViewBag.Title = "Proposal Round";
}

 <!DOCTYPE html>  
 <html>  
 <body>  
   <table id="proposalRound" class="table table-striped table-hover table-condensed">  
     <thead>  
       <tr>  
         <th>WPN</th>  
         <th>Round Number</th>  
         <th>Contractor Type Code</th>  
         <th>PM Code</th>  
         <th>PE Code</th>  
         <th>PS Code</th>  
         <th>Is Westat Prime</th>  
         <th>Westat Sub To</th>  
         <th>Subcontractors</th>  
         <th>Consultants</th>  
         <th>Vendors</th>  
         <th>Round Type Code</th>  
         <th>Due Date</th>  
         <th>Primary Delivery Mode Code</th>  
         <th>Delivery Confirmed Date</th>  
         <th>Due Place</th>  
         <th>SDB Part Plan Required</th>  
         <th>SB Subcontract Plan Required</th>  
         <th>NACIS_SIC_Code</th>  
         <th>Bid Amount</th>  
         <th>Bid Fee Percent</th>  
         <th>Rate Discount</th>  
         <th>SB Percent of Contract</th>  
         <th>SB Percent of Outsourcing</th>  
         <th>Exp. ContractStart Date</th>  
         <th>Exp. Contract End Date</th>  
         <th>Round Status Code</th>  
       </tr>  
     </thead>  
     <tbody id="prList" data-bind="foreach: ProposalRounds">  
 
       <tr> 
        <td class='value editable'>  
           <div class="editor modal">  
             <div class="modal-header">  
               <h3>Edit Proposal Round</h3>  
             </div>  
             <div class="row offset1">  
               <p>  
                 <label>WPN</label>  
                 <input type="text" data-bind="value :WPN" />  
               </p>  
               <p>  
                 <label>Round Number</label>  
                 <input type="text" data-bind="value :RoundNumber" />  
               </p>  
               <p>  
                 <label>Contract Type Code</label>  
                 <input type="text" data-bind="value :ContractTypeCode"></input>                  
               </p>  
               <p>  
                 <label>PMCode</label>  
                 <input type="text" data-bind="value :PMCode"></input>                  
               </p>  
               <p>  
                 <label>PECode</label>  
                 <input type="text" data-bind="value :PECode"></input>                  
               </p>  
               <p>  
                 <label>PSCode</label>  
                 <input type="text" data-bind="value :PSCode"></input>                  
               </p>  
               <p>  
                 <label>Is Westat Prime</label>  
                 <input type="text" data-bind="value :IsWestatPrime"></input>                  
               </p>  
               <p>  
                 <label>WestatSubTo</label>  
                 <input type="text" data-bind="value :WestatSubTo"></input>                  
               </p>  
               <p>  
                 <label>Subcontractors</label>  
                 <input type="text" data-bind="value :Subcontractors"></input>                  
               </p>                
                <p>  
                 <label>Consultants</label>  
                 <input type="text" data-bind="value :Consultants"></input>                  
               </p>                
                <p>  
                 <label>Vendors</label>  
                 <input type="text" data-bind="value :Vendors"></input>                  
               </p>                
                <p>  
                 <label>RoundTypeCode</label>  
                 <input type="text" data-bind="value :RoundTypeCode"></input>                  
               </p>                
                <p>  
                 <label>DueDate</label>  
                 <input type="text" data-bind="value :DueDate"></input>                  
               </p>                
                <p>  
                 <label>PrimaryDeliveryModeCode</label>  
                 <input type="text" data-bind="value :PrimaryDeliveryModeCode"></input>                  
               </p>
                <p>  
                 <label>DeliveryConfirmedDate</label>  
                 <input type="text" data-bind="value :DeliveryConfirmedDate"></input>                  
               </p> 
                 <p>  
                 <label>DuePlace</label>  
                 <input type="text" data-bind="value :DuePlace"></input>                  
               </p>
                <p>  
                 <label>SDBPartPlanRequired</label>  
                 <input type="text" data-bind="value :SDBPartPlanRequired"></input>                  
               </p>
                <!--p>  
                 <label>SBSubcontractPlanRequired</label>       
                 <input type="text" data-bind="value :SBSubcontractPlanRequired"></input>                  
               </p!-->
                <p>  
                 <label>NAICS_SIC_Code</label>  
                 <input type="text" data-bind="value :NAICS_SIC_Code"></input>                  
               </p>
                <p>  
                 <label>BidAmount</label>  
                 <input type="text" data-bind="value :BidAmount"></input>                  
               </p>
                <p>  
                 <label>BidFeePercent</label>  
                 <input type="text" data-bind="value :BidFeePercent"></input>                  
               </p>
                <p>  
                 <label>RateDiscount</label>  
                 <input type="text" data-bind="value :RateDiscount"></input>                  
               </p>
                <p>  
                 <label>SBPercentOfContract</label>  
                 <input type="text" data-bind="value :SBPercentOfContract"></input>                  
               </p>
                <p>  
                 <label>SBPercentOfOutsourcing</label>  
                 <input type="text" data-bind="value :SBPercentOfOutsourcing"></input>                  
               </p>
                <p>  
                 <label>ExpectedContractStartDate</label>  
                 <input type="text" data-bind="value :ExpectedContractStartDate"></input>                  
               </p>
                <p>  
                 <label>ExpectedContractEndDate</label>  
                 <input type="text" data-bind="value :ExpectedContractEndDate"></input>                  
               </p>
                <p>  
                 <label>RoundStatusCode</label>  
                 <input type="text" data-bind="value :RoundStatusCode"></input>                  
               </p>
             </div>  
             <div class="modal-footer">  
               <a href="#" data-dismiss="modal" class="btn proposalRoundCancel">Cancel</a>  
               <a href="#" data-dismiss="modal" class="btn btn-primary proposalRoundSaver">Save</a>  
             </div>  
           </div>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : WPN"></span>
         </td>   
         <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : RoundNumber"></span>  
         </td>          
         <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : ContractTypeCode"></span>  
         </td>
         <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : PMCode"></span>  
         </td>           
         <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : PECode"></span>  
         </td>           
         <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : PSCode"></span>  
         </td>           
         <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : IsWestatPrime"></span>  
         </td>
           <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : WestatSubTo"></span>  
         </td>
           <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : Subcontractors"></span>  
         </td>
           <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : Consultants"></span>  
         </td>
           <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : Vendors"></span>  
         </td>    
            <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : RoundTypeCode"></span>  
         </td>    
           <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text:DueDate"></span>  
         </td>    
           <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : PrimaryDeliveryModeCode"></span>  
         </td>    
           <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : DeliveryConfirmedDate"></span>  
         </td>    
           <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : DuePlace"></span>  
         </td>    
           <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : SDBPartPlanRequired"></span>  
         </td>    
           <td>
    
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="bool : SBSubcontractPlanRequired"></span>  

         </td>    
           <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : NAICS_SIC_Code"></span>  
         </td>    
           <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : BidAmount"></span>  
         </td>    
           <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : BidFeePercent"></span>  
         </td>    
           <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : RateDiscount"></span>  
         </td>    
           <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : SBPercentOfContract"></span>  
         </td>    
           <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : SBPercentOfOutsourcing"></span>  
         </td>    
           <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : ExpectedContractStartDate"></span>  
         </td>    
           <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="Date : ExpectedContractEndDate"></span>  
         </td>    
           <td>  
           <span style="color: darkgrey; font-size: 10pt; font-weight: bold" data-bind="text : RoundStatusCode"></span>  
         </td>    
        <td>  
           <button class="proposalRoundDeleter btn btn-danger">  
             Delete  
           </button>  
         </td>  
       </tr> 
<!-- /ko --> 
     </tbody>  
   </table>
    <div class="pull-right">  
     <a id="new-proposalRound" class="btn btn-success"  
       data-toggle="modal" href="#proposalRound-modal">Add ProposalRound</a>  
    </div>  
   <div class="modal fade" id="proposalRound-modal" style="display: none; ">  
     <div class="modal-header">  
       <button type="button" class="close"  
         data-dismiss="modal">  
         ×</button>  
       <h3>Proposal Round</h3> 
     </div>
     <div class="modal-body"> 
       <div class="row offset1">  
         <p>  
           <label>WPN</label>  
           <input type="text" data-bind="value :newProposalRound().WPN" />  
         </p>  
         <p>  
           <label>Round Number</label>  
           <input type="text" data-bind="value :newProposalRound().RoundNumber" />  
         </p>  
         <p>  
           <label>Contractor Type Code</label>  
           <input type="text" data-bind="value :newProposalRound().ContractTypeCode"></input>  
         </p> 
         <p>  
           <label>PM Code</label>  
           <input type="text" data-bind="value :newProposalRound().PMCode"></input>  
         </p>           
        <p>  
           <label>PE Code</label>  
           <input type="text" data-bind="value :newProposalRound().PECode"></input>  
         </p> 
        <p>  
           <label>PS Code</label>  
           <input type="text" data-bind="value :newProposalRound().PSCode"></input>  
         </p>   
       </div>
    </div>  
       <div class="modal-footer">          
         <a href="#" data-dismiss="modal" class="btn btn-primary proposalRoundAdd">Save</a>  
         <a href="#" data-dismiss="modal" class="btn">Cancel</a>  
       </div>  
       
   </div>      
   @Scripts.Render("~/bundles/ProposalRound")

 </body>  
 </html>  